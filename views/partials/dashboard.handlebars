<article class ="card">
    <header><h1>{{name}}</h1></header>

     <div class="data">
            <div class=" info col-12">
                <h3 class="col-6 text-black">School Webpage</h3>
                <a id='collegePrice' href="{{url}}">{{url}}</a>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Location: </h3>
                <p class="col-6">{{location}}</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Student Size: </h3>
                <p class="col-6">{{size}}</p>
            </div>
            <div class="info col-12">
                <h3 class="col-6 text-black">Tuition</h3>
                <p class="col-6">Instate: {{Instate_Tuition}}</p>
                <p class="col-6">Out of state: {{Out_Of_State_Tuition}}</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Room and Board: </h3>
                <p class="col-6">On Campus: {{On_Campus}}</p>
                <p class="col-6">Off Campus: {{Off_Campus}}</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Acceptance Rate: </h3>
                <p class="col-6">{{Admission_Rate}}%</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Public or Private: </h3>
                <p class="col-6">{{School_Type}}</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Degrees: </h3>
                <p class="col-6">Associate's Degree: {{associates}}</p>
                <p class="col-6">Bachelor's Degree: {{bachelors}}</p>
            </div>
            <div class=" info col-12">
                <h3 class="col-6 text-black">Male to Female Ratio </h3>
                <p class="col-6">M: {{Male_Students}}%</p>
                <p class="col-6">F: {{Female_Students}}%</p>
            </div>
    
</article>

</article>